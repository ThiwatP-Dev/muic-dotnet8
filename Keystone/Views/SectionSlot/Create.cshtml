@{ 
    ViewData["Title"] = "Add Section Slot"; 
}

<section id="data-table-form-v1" class="row">
    <div class="col-lg-12">
        <div class="block block--underline">
            <form class="ks-form container" asp-controller="SectionSlot" asp-action="Create" method="post">
                @Html.AntiForgeryToken()
                <partial name="_Form"/>
            </form>
        </div>
    </div>
</section>

@section Modals {
    <partial name="_DeleteRowConfirmModals" />
}

@section Scripts {
    <script>
        $(document).ready(function() {
            var inputTable = new RowAddAble({
                TargetTable: '#js-table-period',
                ButtonTitle: 'Slot'
            })
            inputTable.RenderButton();
            InputMask.renderTimeMask();
        });
    </script>
}