@model List<RegistrationChangeCourseLogViewModel>

<div id="change-course-log-modal" class="modal fadeIn animated js-details-modal" tabindex="-1" role="dialog"
     aria-hidden="true" data-backdrop="static" data-keyboard="false">
    <div class="modal-dialog modal-xl" role="document">
        <div class="modal-content">
            <div class="block block--underline mb-0">
                <div class="block__title">
                    <header>Modify Registration Course Logs</header>
                </div>
                <div id="modalWrapper-change-course-log-details" class="block__body">
                    <partial name="_Logs"/>
                </div>
                <div class="block__action">
                    <div class="block__action-row">
                        <a href="javascript:void(0);" data-dismiss="modal" class="btn btn-mw-120 btn--cancel rounded">Close</a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>