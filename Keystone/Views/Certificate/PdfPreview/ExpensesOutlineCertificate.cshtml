@model ReportViewModel
@{ 
    ViewData["Title"] = "Certificate Preview";
}

<partial name="PdfPreview/_PreviewHeader" model="@Model" />

@section Scripts {
    <script type="text/javascript" src="~/js/helpers/thai-number.js"></script>
    @if (Model.Language == "en")
    {
        <script type="text/javascript" src="~/js/pages/pdf-render/expenses-outline-certificate-en.js"></script>
    }
    else
    {
        <script type="text/javascript" src="~/js/pages/pdf-render/expenses-outline-certificate-th.js"></script>
    }
}