@model ReportViewModel
@{ 
    ViewData["Title"] = "Certificate Preview";
}

<partial name="PdfPreview/_PreviewHeader" model="@Model" />

@section Scripts {
    @if (Model.Language == "en")
    {
        <script type="text/javascript" src="~/js/pages/pdf-render/graduating-waiting-grade-certification-en.js"></script>
    }
    else
    {
        <script type="text/javascript" src="~/js/pages/pdf-render/graduating-waiting-grade-certification-th.js"></script>
    }
}