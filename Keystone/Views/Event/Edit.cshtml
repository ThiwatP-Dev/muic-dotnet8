@model Event
@{ 
    ViewData["Title"] = "Edit Event"; 
}

<section id="data-table-form-v1" class="row">
    <div class="col-lg-12">
        <div class="block block--underline">
            <form class="ks-form container" asp-controller="Event" asp-action="Edit" method="post">
