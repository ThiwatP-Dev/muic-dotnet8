@model ParentInformation

<div id="create-modal-parent" class="modal fadeIn animated js-details-modal" tabindex="-1" role="dialog"
     aria-hidden="true" data-backdrop="static" data-keyboard="false">
    <div class="modal-dialog modal-dialog-centered modal-lg" role="document">
        <div class="modal-content">
            <div class="block block--underline mb-0">
                <div class="block__title">
                    <header>Add Parent</header>
                </div>
                <div class="block__body">
                    <div id="modalWrapper-parent-create">
                        <form class="ks-form container py-0" asp-controller="Parent" asp-action="Create" asp-route-page="@ViewData["Page"]" method="POST">
                            @Html.AntiForgeryToken()
                            <partial name="_Form.cshtml"/>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>