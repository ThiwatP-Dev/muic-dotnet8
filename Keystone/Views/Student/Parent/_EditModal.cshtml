@model ParentInformation

<div id="edit-parent-modal" class="modal fadeIn animated js-details-modal" tabindex="-1" role="dialog"
    aria-hidden="true" data-backdrop="static" data-keyboard="false">
    <div class="modal-dialog modal-dialog-centered modal-lg" role="document">
        <div class="modal-content">
            <div class="block block--underline mb-0">
                <div class="block__title">
                    <header>Edit Parent</header>
                </div>
                <div class="block__body">
                    <form class="ks-form container py-0" asp-controller="Parent" asp-action="Edit" asp-route-page="@ViewData["Page"]" method="POST">
                        @Html.AntiForgeryToken()
                        <div id="modalWrapper-parent-edit">
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>