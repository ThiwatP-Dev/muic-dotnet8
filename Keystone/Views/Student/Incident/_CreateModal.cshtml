@model StudentIncident

<div id="create-modal-incident" class="modal fadeIn animated js-modal-form" tabindex="-1" role="dialog"
     aria-hidden="true" data-backdrop="static" data-keyboard="false">
	<div class="modal-dialog modal-dialog-centered modal-lg" role="document">
		<div class="modal-content">
			<div class="block block--underline mb-0">
				<div class="block__title">
					<header>Add Incident</header>
				</div>
				<div class="block__body">
					<form class="ks-form container py-0" asp-controller="StudentIncident" asp-action="Create" method="POST">
						@Html.AntiForgeryToken()
						<partial name="~/Views/Student/Incident/_Form.cshtml" model="@Model"/>
					</form>
				</div>
			</div>
		</div>
	</div>
</div>