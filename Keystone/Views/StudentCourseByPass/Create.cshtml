@model StudentCourseByPassViewModel
@{ 
    ViewData["Title"] = "Add Student Course By Pass"; 
}

<section class="row">
    <div class="col-12">
        <form class="form-horizontal" asp-controller="StudentCourseByPass" asp-action="Create" method="post">
            @Html.AntiForgeryToken()
            <partial name="_Form"/>
        </form>
    </div>
</section>

@section Modals {
    <partial name="_DeleteRowConfirmModals">
}

@section Scripts {
    <script>
        $(document).ready( function() {
            var table = new RowAddAble({
                TargetTable: "#js-add-course",
                TableTitle: 'Course',
                ButtonTitle: 'Course'
            });
            table.RenderButton();
        });
    </script>
}