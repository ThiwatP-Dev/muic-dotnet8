@model VerificationLetterViewModel

<section class="row">
    <div class="ks-page-header">
        <h2 class="ks-page-header__title">Verification Letter</h2>
    </div>
</section>

<section id="verification-letter-preview" data-model="@JsonConvert.SerializeObject(Model)">
    <div class="block block__body">
        <iframe id="js-print-preview" class="w-100x h-85vh"></iframe>
    </div>
</section>

<section>
    <a class="btn btn-mw-120 btn--cancel" asp-action="Index">Finish</a>
</section>

@section Scripts {
    <script type="text/javascript" src="~/js/pages/pdf-render/verification-letter-admission-th.js"></script>
}