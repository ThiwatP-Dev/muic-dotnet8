@model ScholarshipStudent

<div id="add-scholarship-student-modal" class="modal fadeIn animated js-modal-form js-scholarship-modal" tabindex="-1" role="dialog"
     aria-hidden="true" data-backdrop="static" data-keyboard="false">
	<div class="modal-dialog modal-dialog-centered modal-lg" role="document">
		<div class="modal-content">
			<div class="block block--underline mb-0">
				<div class="block__title">
					<header>Add Scholarship Student</header>
				</div>
				<div class="block__body">
					<form class="ks-form container py-0" asp-controller="ScholarshipProfile" asp-action="AddScholarshipStudent" method="POST">
						<partial name="_Form">
					</form>
				</div>
			</div>
		</div>
	</div>
</div>