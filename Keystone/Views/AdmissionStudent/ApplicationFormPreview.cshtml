@model ApplicationFormViewModel
@{ 
    ViewData["Title"] = "Application Form Preview";
}

<section class="row">
    <div class="ks-page-header">
        <a class="btn-back" href="@ViewBag.ReturnUrl">
            <i class="la la-angle-double-left mr-1"></i>
        </a>
        <h2 class="ks-page-header__title">@ViewData["Title"]</h2>
    </div>
</section>

<partial name="_PdfPreview" model="@Model">

<section>
    <div class="block__action-row pr-0">
        <a class="btn btn-mw-120 btn--success" asp-action="Index">Finish</a>
    </div>
</section>

@section Scripts {
    <script type="text/javascript" src="~/js/pages/pdf-render/application-form.js"></script>
}